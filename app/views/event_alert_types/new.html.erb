<div class="note Event Alert">
<h2>Create a new Event Alert</h2>
<%= form_for(:note, :url=>{:action=>"create"}) do |f| %>




	<%= render(:partial=>"form", :locals=>{:f=>f}) %>


	<p> * fields marked with asterisk are compulsary fields</p>

		<div class="form_buttons">

		<%=submit_tag("Create Event Alert") %>

		</div>
<% end -%>
</div>

<div id="errors" style="color:red">
	<% @note.errors.full_messages.each do |err| %>
    	*<%= err %><br />
    <% end %>
</div>