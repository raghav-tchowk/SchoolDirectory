<% if !flash[:notice].to_i.blank? %>
<div>
<%= flash[:notice] %>
</div>
<% end -%>
	
	

<div>
	<h2>Exam Type</h2>



	<table summary="examtypes list">
		<tr>
		<th>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</th>
		<th>ExamType&nbsp;&nbsp;&nbsp;&nbsp;</th>
		<th>Details&nbsp;&nbsp;&nbsp;&nbsp;</th>
		
		</tr>
		<% @examtypes.each do |examtype| %>

			<tr>
				<td><%=examtype.id%></td>
				<td><%=examtype.name%></td>
				<td><%=examtype.detail%></td>
				
				<td>

					<%=link_to("Show", {:action=>"show", :id=>examtype.id}) %>
				<%= link_to("Edit", {:action=>"edit", :id=>examtype.id}) %>
				<%= link_to("Delete", {:action=>"delete", :id=>examtype.id}) %>



				</td>


			</tr>
		<% end %>
	</table>
</div>

<%= link_to("<<new ExamType>>", {:action=>"new"}) %>

